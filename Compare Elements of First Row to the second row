#include <iostream>
using namespace std;
int main(){
    int r,c;
    cout<<"Enter number of rows: ";
    cin>>r;
    cout<<"Enter number of columns: ";
    cin>>c;
    int m[50][50];
    cout<<"\nEnter elements of matrix:\n";
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<"Element ["<<i<<"]["<<j<<"]: ";
            cin>>m[i][j];
        }
    }
    if(r < 2){
        cout<<"Matrix does not have two rows to compare.";
        return 0;
    }
    cout<<"Comparing first row and second row:\n";
    for(int j=0;j<c;j++){
        if(m[0][j] == m[1][j]){
            cout<<"Column "<<j<<": Equal ("<<m[0][j]<<")\n";
        } else {
            cout<<"Column "<<j<<": Not Equal ("<<m[0][j]<<" vs "<<m[1][j]<<")\n";
        }
    }
    return 0;
}
/*Output:-
Enter number of rows: 3
Enter number of columns: 3

Enter elements of matrix:
Element [0][0]: 1
Element [0][1]: 2
Element [0][2]: 3
Element [1][0]: 4
Element [1][1]: 5
Element [1][2]: 6
Element [2][0]: 7
Element [2][1]: 8
Element [2][2]: 9
Comparing first row and second row:
Column 0: Not Equal (1 vs 4)
Column 1: Not Equal (2 vs 5)
Column 2: Not Equal (3 vs 6)
*/
